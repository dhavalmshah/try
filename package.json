      "@feathersjs/socketio": "^4.5.11",
      "@feathersjs-objection": "^6.1.0",
      "dotenv": "^10.0.0",
      "express": "^4.17.1",
      "helmet": "^4.6.0",
      "knex": "^0.95.6",
